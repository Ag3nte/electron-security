{
  "name": "electron-security",
  "version": "1.0.0-SNAPSHOT",
  "description": "An App for learning",
  "main": "main.js",
  "module": "_main.js",
  "scripts": {
    "start": "electron .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "rebuild": "electron-rebuild -f -w sqlite3",
    "rebuild-alias": "electron-rebuild -f -w module-alias",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps"
  },
  "author": "Ag3nte",
  "license": "MIT",
  "devDependencies": {
    "electron": "^7.1.9",
    "electron-builder": "^22.4.0",
    "electron-rebuild": "^1.10.0"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        }
      ]
    }
  },
  "build": {
    "appId": "com.electron.electron-security",
    "files": [
      "**/*"
    ]
  },

  "dependencies": {
    "electron-reload": "^1.5.0",
    "module-alias": "^2.2.2",
    "sqlite3": "^4.1.1"
  },
  "_moduleAliases": {
    "@component": "./engine/component.js",
    "@app": "./app",
    "@manager": "./db/manager.js",
    "@services": "./db/services"
  },
  "keywords": []
}
